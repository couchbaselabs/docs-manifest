<manifest>
   <remote name="couchbase"     fetch="git://github.com/couchbase/" />
   <remote name="couchbase-priv" fetch="ssh://git@github.com/couchbase/" />
   <default revision="release/3.0"
       remote="couchbase"
           sync-j="4" />
    <project name="docs-sdk-common" revision="release/6.5" />
    <project name="docs-sdk-c" />
    <project name="docs-sdk-dotnet" />
    <project name="docs-sdk-go" />
    <project name="docs-sdk-java" />
    <project name="docs-sdk-nodejs" />
    <project name="docs-sdk-php" />
    <project name="docs-sdk-python" />
    <project name="docs-sdk-ruby" remote="couchbase-priv" />
    <project name="docs-sdk-scala" />
 </manifest>
